{
  "version": 1.1,
  "locale": {
    "source": "en",
    "targets": [
      "es",
      "fr",
      "hi",
      "ja",
      "ko",
      "de",
      "ar",
      "ru",
      "zh",
      "pt-BR"
    ]
  },
  "buckets": {
    "markdown": {
      "include": [
        "readme/[locale].md"
      ]
    },
    "json": {
      "include": [
        "i18n/[locale].json"
      ]
    }
  }
}