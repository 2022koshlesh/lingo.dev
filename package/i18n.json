{
  "version": 1.1,
  "locale": {
    "source": "en",
    "targets": [
      "es",
      "fr",
      "de",
      "ru"
    ]
  },
  "buckets": {
    "markdown": {
      "include": [
        "demo/markdown-multi/[locale]/*.md",
        "demo/markdown/[locale].md"
      ],
      "exclude": [
        "demo/markdown-multi/[locale]/ignored-blog-post.md"
      ]
    },
    "json": {
      "include": ["demo/json/[locale].json"]
    }
  }
}
