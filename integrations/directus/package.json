{
  "name": "@replexica/integration-directus",
  "version": "0.1.0",
  "description": "Replexica integration for Directus",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "types": "build/index.d.ts",
  "module": "build/index.mjs",
  "main": "build/index.cjs",
  "directus:extension": {
    "type": "operation",
    "path": {
      "app": "build/app.mjs",
      "api": "build/api.mjs"
    },
    "source": {
      "app": "src/app.ts",
      "api": "src/api.ts"
    },
    "host": "^10.10.0"
  },
  "files": [
    "build",
    "readme.md",
    "changelog.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsc --noEmit && tsup",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@replexica/sdk": "workspace:*"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "12.1.4",
    "tsup": "^8.3.5",
    "vitest": "^2.1.4"
  }
}
